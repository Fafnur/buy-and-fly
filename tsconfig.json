/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": false,
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "lib": ["ES2022", "dom"],
    "baseUrl": ".",
    "paths": {
      "@baf/core": ["src/app/core/index.ts"],
      "@baf/development/page": ["src/app/development/page/index.ts"],
      "@baf/errors/not-found/page": ["src/app/errors/not-found/page/index.ts"],
      "@baf/errors/permission-denied/page": ["src/app/errors/permission-denied/page/index.ts"],
      "@baf/errors/server-error/page": ["src/app/errors/server-error/page/index.ts"],
      "@baf/errors/ui/links": ["src/app/errors/ui/links/index.ts"],
      "@baf/home/page": ["src/app/home/page/index.ts"],
      "@baf/search/avia/common": ["src/app/search/avia/common/index.ts"],
      "@baf/search/avia/ui/form": ["src/app/search/avia/ui/form/index.ts"],
      "@baf/search/common": ["src/app/search/common/index.ts"],
      "@baf/search/hotels/common": ["src/app/search/hotels/common/index.ts"],
      "@baf/search/hotels/ui/form": ["src/app/search/hotels/ui/form/index.ts"],
      "@baf/search/page": ["src/app/search/page/index.ts"],
      "@baf/search/railways/common": ["src/app/search/railways/common/index.ts"],
      "@baf/search/railways/ui/form": ["src/app/search/railways/ui/form/index.ts"],
      "@baf/search/services": ["src/app/search/services/index.ts"],
      "@baf/search/ui/fields": ["src/app/search/ui/fields/index.ts"],
      "@baf/search/ui/form": ["src/app/search/ui/form/index.ts"],
      "@baf/search/ui/layout": ["src/app/search/ui/layout/index.ts"],
      "@baf/ui/accordion": ["src/app/ui/accordion/index.ts"],
      "@baf/ui/autocomplete": ["src/app/ui/autocomplete/index.ts"],
      "@baf/ui/buttons": ["src/app/ui/buttons/index.ts"],
      "@baf/ui/cards": ["src/app/ui/cards/index.ts"],
      "@baf/ui/container": ["src/app/ui/container/index.ts"],
      "@baf/ui/datepicker": ["src/app/ui/datepicker/index.ts"],
      "@baf/ui/headline": ["src/app/ui/headline/index.ts"],
      "@baf/ui/icons": ["src/app/ui/icons/index.ts"],
      "@baf/ui/input": ["src/app/ui/input/index.ts"],
      "@baf/ui/label": ["src/app/ui/label/index.ts"],
      "@baf/ui/layout": ["src/app/ui/layout/index.ts"],
      "@baf/ui/nav": ["src/app/ui/nav/index.ts"],
      "@baf/ui/section": ["src/app/ui/section/index.ts"],
      "@baf/ui/title": ["src/app/ui/title/index.ts"],
      "@baf/ui/utils": ["src/app/ui/utils/index.ts"]
    }
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
